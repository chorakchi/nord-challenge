{
  "name": "nord-app",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "sb": "start-storybook -s ./public -p 6006",
    "build-sb": "build-storybook -s ./public",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-coverage": "test --coverage  --watchAll=false"
  },
  "dependencies": {
    "next": "12.0.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "use-http": "^1.0.26"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.3.12",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.2",
    "@types/react": "17.0.34",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "babel-preset-react-app": "^10.0.0",
    "enzyme": "^3.11.0",
    "eslint": "7",
    "eslint-config-next": "12.0.3",
    "jest": "^27.3.1",
    "postcss": "^8.3.11",
    "react-test-renderer": "^17.0.2",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.4.4"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/index.tsx",
      "!*/index.tsx",
      "!src/**/*.stories.tsx",
      "!src/**/*.type.tsx",
      "!src/**/*.interface.tsx",
      "!src/**/*.interface.ts",
      "!src/**/*.mock.tsx",
      "!src/**/*.mock.ts",
      "!src/**/*.styled.tsx",
      "!src/**/*.styled.ts",
      "!src/**/iconList.tsx",
      "!*/react-app-env.d.ts"
    ]
  }
}
